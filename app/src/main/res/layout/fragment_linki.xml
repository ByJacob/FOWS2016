<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                tools:context="pl.kebapp.byjacob.fows2016.FragmentyGlowne.Linki"
                android:background="@android:color/background_light">

    <!-- TODO: Update blank fragment layout -->

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="50dp"
        app:srcCompat="@drawable/naglowek"
        android:id="@+id/imageView12"
        android:adjustViewBounds="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:background="@drawable/tlonaglowek"/>

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="50dp"
        app:srcCompat="@color/transparent"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:id="@+id/open_kebapp"/>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true">

        <TextView
            android:id="@+id/textViewWirelessGroup"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:text="WirelessGroup"
            android:textAppearance="@android:style/TextAppearance.DeviceDefault.Medium"
            android:textSize="36sp"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true"
            android:layout_marginStart="10dp"
            android:textAlignment="center"/>

        <ImageView
            android:id="@+id/imageView15"
            android:layout_width="match_parent"
            android:layout_height="5dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            app:srcCompat="?attr/colorControlHighlight"
            android:layout_alignBottom="@+id/textViewWirelessGroup"/>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="5dp"
            android:paddingBottom="5dp">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/internet_icon"
                android:id="@+id/wg_normal"
                android:layout_weight="1"
                android:maxHeight="50dp"
                android:adjustViewBounds="true"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"/>

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/fb_icon"
                android:id="@+id/wg_fb"
                android:layout_weight="1"
                android:adjustViewBounds="true"
                android:maxHeight="50dp"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"/>

        </LinearLayout>

        <ImageView
            android:id="@+id/imageView151"
            android:layout_width="match_parent"
            android:layout_height="3dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            app:srcCompat="?attr/colorControlHighlight"
            android:layout_alignBottom="@+id/wg_fb"/>

        <TextView
            android:id="@+id/textViewFOWS"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:text="@string/app_full_name"
            android:textAppearance="@android:style/TextAppearance.DeviceDefault.Medium"
            android:textSize="36sp"
            android:layout_below="@+id/wg_fb"
            android:layout_marginStart="10dp"
            android:paddingTop="10dp"
            android:textAlignment="center"/>

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="match_parent"
            android:layout_height="5dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            app:srcCompat="?attr/colorControlHighlight"
            android:layout_alignBottom="@+id/textViewFOWS"/>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="5dp"
            android:paddingBottom="5dp">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/internet_icon"
                android:id="@+id/fows_normal"
                android:layout_weight="1"
                android:maxHeight="50dp"
                android:adjustViewBounds="true"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"/>

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/fb_icon"
                android:id="@+id/fows_fb"
                android:layout_weight="1"
                android:adjustViewBounds="true"
                android:maxHeight="50dp"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"/>

        </LinearLayout>

        <ImageView
            android:id="@+id/imageView152"
            android:layout_width="match_parent"
            android:layout_height="3dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            app:srcCompat="?attr/colorControlHighlight"
            android:layout_alignBottom="@+id/fows_fb"/>

    </LinearLayout>

</RelativeLayout>
